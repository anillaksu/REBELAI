modules = ["python-3.11", "nodejs-20", "postgresql-16"]
[agent]
expertMode = true
integrations = ["python_openai:1.0.0", "javascript_openai:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["libyaml", "xdotool", "wmctrl"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "REBEL AI Portable"

[[workflows.workflow]]
name = "REBEL AI Portable"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd REBEL_AI_Portable && node src/server.js --portable"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80
exposeLocalhost = true

[[ports]]
localPort = 37043
externalPort = 3000

[deployment]
deploymentTarget = "cloudrun"
