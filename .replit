modules = ["python-3.11"]
[agent]
expertMode = true
integrations = ["python_openai:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["libyaml"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "REBEL AI Server"

[[workflows.workflow]]
name = "REBEL AI Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "gunicorn -w 2 -b 0.0.0.0:5000 --timeout 60 rebel_ai_manager:app"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 41495
externalPort = 3000

[[ports]]
localPort = 42601
externalPort = 3001
